

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
   <servlet>
 	 <servlet-name>CreateStudent</servlet-name>
  	<servlet-class>com.ty.controller.CreateStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>CreateStudent</servlet-name>
  	<url-pattern>/creates</url-pattern>
  </servlet-mapping>
  
  <servlet>
 	 <servlet-name>DisplayStudent</servlet-name>
  	<servlet-class>com.ty.controller.DisplayStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>DisplayStudent</servlet-name>
  	<url-pattern>/displays</url-pattern>
  </servlet-mapping>
  
  <servlet>
 	 <servlet-name>DeleteStudent</servlet-name>
  	<servlet-class>com.ty.controller.DeleteStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>DeleteStudent</servlet-name>
  	<url-pattern>/deletes</url-pattern>
  </servlet-mapping>
  
   <servlet>
 	 <servlet-name>GetStudents</servlet-name>
  	<servlet-class>com.ty.controller.GetStudents</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>GetStudents</servlet-name>
  	<url-pattern>/getstudents</url-pattern>
  </servlet-mapping>
   <servlet>
 	 <servlet-name>EditStudent</servlet-name>
  	<servlet-class>com.ty.controller.EditStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>EditStudent</servlet-name>
  	<url-pattern>/edits</url-pattern>
  </servlet-mapping>
  
  	<servlet>
  		<servlet-name>Login</servlet-name>
  		<servlet-class>com.ty.controller.LoginServlet</servlet-class>
  	</servlet>
  	<servlet-mapping>
  		<servlet-name>Login</servlet-name>
  		<url-pattern>/login</url-pattern>
  	</servlet-mapping>
  	<servlet>
  		<servlet-name>Logout</servlet-name>
  		<servlet-class>com.ty.controller.LogoutServlet</servlet-class>
  	</servlet>
  	<servlet-mapping>
  		<servlet-name>Logout</servlet-name>
  		<url-pattern>/logout</url-pattern>
  	</servlet-mapping>
</web-app>